# BOW Backend Environment Configuration
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=development
PORT=3000

# AWS Configuration
AWS_REGION=us-west-2
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Cognito Configuration
COGNITO_USER_POOL_ID=us-west-2_Imazy2DXa
COGNITO_CLIENT_ID=7qiar42f9ujh3p8atoel4dp055
COGNITO_REGION=us-west-2

# DynamoDB Configuration
DYNAMODB_TABLE_PREFIX=bow_

# S3 Configuration
S3_BUCKET_NAME=bow-media-storages
S3_REGION=us-west-2

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Security Configuration
JWT_SECRET=your_jwt_secret_key_here_make_it_long_and_random
SESSION_SECRET=your_session_secret_here_make_it_unique

# CORS Configuration (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,https://yourdomain.com

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# API Configuration
API_BASE_URL=https://your-api-domain.com

# Email Configuration (if using SES)
SES_REGION=us-west-2
SES_ACCESS_KEY_ID=your_ses_access_key_id
SES_SECRET_ACCESS_KEY=your_ses_secret_access_key

# Database Configuration (if using other databases)
DATABASE_URL=your_database_url

# Redis Configuration (if using Redis)
REDIS_URL=your_redis_url

# Monitoring and Analytics
SENTRY_DSN=your_sentry_dsn
NEW_RELIC_LICENSE_KEY=your_new_relic_key

# Feature Flags
ENABLE_REGISTRATION=true
ENABLE_EMAIL_VERIFICATION=true
ENABLE_STRIPE_PAYMENTS=true

# Development/Testing
ENABLE_MOCK_DATA=false
SKIP_AUTHENTICATION=false

